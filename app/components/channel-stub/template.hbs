{{#link-to "index" (query-params channel=channel.id) class="channel-stub-link"}}
  <p class="channel-stub-title">
    {{siteName}}
  </p>
  <div class="channel-stub-logo">
    {{#if logo}}
      <img src={{logo}} class="img img-responsive" alt="Logo">
    {{else}}
      <img
        src="{{rootURL}}images/temp-peg-logo.png"
        class="img img-responsive" alt="Logo"
      >
    {{/if}}
  </div>
  {{!--<p>{{{channel.publicSite.aboutPageDescription}}}</p>--}}
{{/link-to}}