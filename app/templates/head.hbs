{{!
  Add content you wish automatically added to the documents head
  here. The 'model' available in this template can be populated by
  setting values on the 'head-data' service.
}}
<link
  data-channel-colors
  rel="stylesheet"
  href="{{this.model.rootURL}}colors-{{this.model.channelID}}.css"
>
<link
  data-channel-custom
  rel="stylesheet"
  href="{{this.model.rootURL}}custom-{{this.model.channelID}}.css"
>
<meta name="twitter:card" content={{this.model.socialMedia.card}} >
{{!-- template-lint-disable no-unnecessary-concat --}}
<meta name="twitter:title" content="{{this.model.socialMedia.title}}" >
<meta name="twitter:description" content="{{this.model.socialMedia.description}}" >
{{#if this.model.socialMedia.image}}
  <meta name="twitter:image" content="{{this.model.socialMedia.image}}" >
{{/if}}
<meta property="og:type" content={{this.model.socialMedia.type}} >
<meta property="og:title" content={{this.model.socialMedia.title}} >
{{#if this.model.socialMedia.description}}
  <meta property="og:description" content={{this.model.socialMedia.description}} >
{{/if}}
{{#if this.model.socialMedia.image}}
  <meta property="og:image" content={{this.model.socialMedia.image}} >
{{/if}}
<meta property="og:url" content={{this.model.url}} >
{{#if this.model.jsonLD}}
  <script type="application/ld+json">
    {{this.model.jsonLD}}
  </script>
{{/if}}